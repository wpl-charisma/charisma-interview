{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "prettier"
    ],
    "plugins": ["prettier", "@typescript-eslint", "react-hooks"],
    "ignorePatterns": ["node_modules/", "*.js", ".expo/*"],
    "rules": {
        "react/react-in-jsx-scope": "off"
    }
}